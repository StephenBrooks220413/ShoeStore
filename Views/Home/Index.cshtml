@{
    ViewData["Title"] = "Home Page";
}

    <center>
        <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
            <div class="carousel-inner">
                <div class="carousel-item active">
                    <img src="https://images.unsplash.com/photo-1479064555552-3ef4979f8908?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1350&q=80" class="d-block w-100" height="310px;" alt="...">
                </div>
                <div class="carousel-item">
                    <img src="https://images.unsplash.com/photo-1542702942-01343dd60a84?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1350&q=80" class="d-block w-100" height="310px;" alt="...">
                </div>
                <div class="carousel-item">
                    <img src="https://images.unsplash.com/photo-1513373319109-eb154073eb0b?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1350&q=80" class="d-block w-100" height="310px;" alt="...">
                </div>
            </div>
            <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
                <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                <span class="sr-only">Previous</span>
            </a>
            <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
                <span class="carousel-control-next-icon" aria-hidden="true"></span>
                <span class="sr-only">Next</span>
            </a>
        </div>
    </center>

    <div class="text-center" style="margin-top:90px;">
        <h1 class="display-4">Welcome</h1>
        <p class="lead">Making Online Buying Easy</p>
    </div>

    <div class="container" style="margin-top:40px;">
        <div class="row">
            <div class="col-md-4 text-center" style="margin-top:7px;">
                <label>Kid's Shoes</label> <br />
                <img src="https://images.unsplash.com/photo-1495021146843-ff31dd6121b3?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1350&q=80" width="95%" alt="Kid Shoes" />
            </div>
            <div class="col-md-4 text-center" style="margin-top:7px;">
                <label>Women Shoes</label> <br />
                <img src="https://images.unsplash.com/photo-1512664401326-cd1a54ccd52d?ixlib=rb-1.2.1&ixid=eyJhcHBfaWQiOjEyMDd9&auto=format&fit=crop&w=1350&q=80" width="95%" alt="Women Shoes" />
            </div>
            <div class="col-md-4 text-center" style="margin-top:7px;">
                <label>Men's shoes</label> <br />
                <img src="https://images.unsplash.com/photo-1451680350250-2ae7d6e00f5a?ixlib=rb-1.2.1&auto=format&fit=crop&w=1350&q=80" width="95%" alt="Men Shoes" />
            </div>
        </div>
    </div>

    <article style="margin-top:70px;">
        <div class="container media-wrapper">
            <div class="row">
                <div class="col-md">
                    <div class="media-1 text-center">
                        <i class="fab fa-facebook"></i>
                    </div>
                </div>
                <div class="col-md">
                    <div class="media-2 text-center">
                        <i class="fab fa-twitter"></i>
                    </div>
                </div>
                <div class="col-md">
                    <div class="media-3 text-center">
                        <i class="fab fa-instagram"></i>
                    </div>
                </div>
                <div class="col-md">
                    <div class="media-4 text-center">
                        <i class="fas fa-photo-video"></i>
                    </div>
                </div>
            </div>
        </div>
    </article>
